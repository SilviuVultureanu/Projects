    "import pandas as pd\n",
    "import glob\n",
    "\n",
    "student_files = glob.glob('exams*.csv')\n",
    "\n",
    "df_list = []\n",
    "for exams in student_files:\n",
    "  data = pd.read_csv(exams)\n",
    "  df_list.append(data)\n",
    "\n",
    "students = pd.concat(df_list)\n",
    "students\n",
    "#print(len(students))\n",
    "#df_list"
   ]
  }
